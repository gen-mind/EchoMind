{
  "id": null,
  "uid": "ragas-evaluation",
  "title": "RAGAS Evaluation",
  "description": "RAG quality scores from RAGAS evaluation: faithfulness, response relevancy, context precision",
  "tags": ["echomind", "ragas", "evaluation"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "Score Trends",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "title": "Faithfulness (5m avg)",
      "description": "Average faithfulness score over 5-minute windows",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_faithfulness_score_sum[5m]) / rate(ragas_faithfulness_score_count[5m])",
          "legendFormat": "faithfulness",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "spanNulls": true
          },
          "unit": "percentunit"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 2,
      "title": "Response Relevancy (5m avg)",
      "description": "Average response relevancy score over 5-minute windows",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_response_relevancy_score_sum[5m]) / rate(ragas_response_relevancy_score_count[5m])",
          "legendFormat": "response_relevancy",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "spanNulls": true
          },
          "unit": "percentunit"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 3,
      "title": "Context Precision (5m avg)",
      "description": "Average context precision score over 5-minute windows",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_context_precision_score_sum[5m]) / rate(ragas_context_precision_score_count[5m])",
          "legendFormat": "context_precision",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "spanNulls": true
          },
          "unit": "percentunit"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 101,
      "title": "Current Averages",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 4,
      "title": "Faithfulness",
      "description": "Current average faithfulness score",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_faithfulness_score_sum[1h]) / rate(ragas_faithfulness_score_count[1h])",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.4 },
              { "color": "green", "value": 0.7 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 5,
      "title": "Response Relevancy",
      "description": "Current average response relevancy score",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_response_relevancy_score_sum[1h]) / rate(ragas_response_relevancy_score_count[1h])",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.4 },
              { "color": "green", "value": 0.7 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 6,
      "title": "Context Precision",
      "description": "Current average context precision score",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ragas_context_precision_score_sum[1h]) / rate(ragas_context_precision_score_count[1h])",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.4 },
              { "color": "green", "value": 0.7 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 102,
      "title": "Evaluation Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 7,
      "title": "Evaluations per Hour",
      "description": "Rate of RAGAS evaluations by status",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "increase(ragas_evaluations_total{status=\"success\"}[1h])",
          "legendFormat": "success",
          "refId": "A"
        },
        {
          "expr": "increase(ragas_evaluations_total{status=\"error\"}[1h])",
          "legendFormat": "error",
          "refId": "B"
        },
        {
          "expr": "increase(ragas_evaluations_total{status=\"skipped\"}[1h])",
          "legendFormat": "skipped",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 30,
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "error" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "skipped" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 8,
      "title": "Evaluation Duration",
      "description": "RAGAS evaluation latency (p50, p90, p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, rate(ragas_evaluation_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.9, rate(ragas_evaluation_duration_seconds_bucket[5m]))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(ragas_evaluation_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 103,
      "title": "Score Distributions",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 9,
      "title": "Faithfulness Distribution",
      "description": "Histogram of faithfulness scores",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 26 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "increase(ragas_faithfulness_score_bucket[6h])",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Greens" },
          "unit": "short"
        }
      },
      "options": {
        "bucketOffset": 0,
        "combine": false,
        "fillOpacity": 80
      }
    },
    {
      "id": 10,
      "title": "Response Relevancy Distribution",
      "description": "Histogram of response relevancy scores",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 26 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "increase(ragas_response_relevancy_score_bucket[6h])",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Blues" },
          "unit": "short"
        }
      },
      "options": {
        "bucketOffset": 0,
        "combine": false,
        "fillOpacity": 80
      }
    },
    {
      "id": 11,
      "title": "Context Precision Distribution",
      "description": "Histogram of context precision scores",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 26 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "increase(ragas_context_precision_score_bucket[6h])",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Purples" },
          "unit": "short"
        }
      },
      "options": {
        "bucketOffset": 0,
        "combine": false,
        "fillOpacity": 80
      }
    }
  ]
}
