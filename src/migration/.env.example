# EchoMind Migration Service Configuration
# Copy to .env and fill in your values for local development

# Database (REQUIRED - sync driver for Alembic)
# Format: postgresql://user:password@host:port/database
MIGRATION_DATABASE_URL=postgresql://echomind:changeme@localhost:5432/echomind

# Migration settings
MIGRATION_RETRY_COUNT=5
MIGRATION_RETRY_DELAY=5

# Logging
MIGRATION_LOG_LEVEL=INFO
